<p id="notice"><%= notice %></p>

<h1>Imports</h1>

<table>
	<thead>
		<tr>
			<th>All imports</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @imports.each do |import| %>
		<tr>
			<td><%= import.title %></td>
			<td><%= link_to 'Show', import %></td>
      <td><%= link_to 'Edit', edit_import_path(import) %></td>
      <td><%= link_to 'Delete Import', import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<div>
	<%= link_to 'Create Import', new_import_path %>
</div>
